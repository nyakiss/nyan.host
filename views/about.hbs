{{#each about}}
	<ul class="rate">
		<h2>{{title}}</h2>
		{{#each items}}
			<li>
				<span class="cpu">{{cpu}}</span>
				<span class="ram">{{ram}}</span>
        <span class="rom">{{rom}}</span>
				<span class="prs">{{prs}}₽</span>
				{{#if 2prs}}<span class="2prs">{{2prs}}₽</span>{{/if}}
			</li>
		{{/each}}
	</ul>
{{/each}}
